import { httpService } from '../util/axios'

export default {
  getShopList(data){
    return httpService.post('/shops/getShopList', data || {})
  },
  getShopMenu(data){
    return httpService.post('/shops/getShopMenu', data || {})
  },
  getShopInfo(data){
    return httpService.post('/shops/getShopInfo', data || {})
  }
}
